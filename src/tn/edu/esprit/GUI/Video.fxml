<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.edu.esprit.GUI.VideoController">
   <center>
      <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <MediaView fx:id="mediaView" fitHeight="200.0" fitWidth="200.0" />
            <VBox alignment="BOTTOM_CENTER" maxHeight="100.0" minHeight="100.0" prefHeight="100.0" prefWidth="100.0" StackPane.alignment="BOTTOM_CENTER">
               <children>
                  <Slider fx:id="progressBar" prefHeight="18.0" prefWidth="606.0">
                     <VBox.margin>
                        <Insets right="10.0" />
                     </VBox.margin>
                  </Slider>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#choisir" text="Button">
                           <HBox.margin>
                              <Insets left="3.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="lireVideo" mnemonicParsing="false" onAction="#lire" text="Lire">
                           <HBox.margin>
                              <Insets left="3.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="pauseVideo" mnemonicParsing="false" onAction="#pause" text="Pause">
                           <HBox.margin>
                              <Insets left="3.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="stopvideo" mnemonicParsing="false" onAction="#stop" text="Stop">
                           <HBox.margin>
                              <Insets left="3.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="speedVideo" mnemonicParsing="false" onAction="#speed" text="&lt;&lt;&lt;">
                           <HBox.margin>
                              <Insets left="3.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="slowVedio" mnemonicParsing="false" onAction="#slow" text="&gt;&gt;&gt;">
                           <HBox.margin>
                              <Insets left="3.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="backVideo" mnemonicParsing="false" onAction="#back10" text="-10s">
                           <HBox.margin>
                              <Insets left="3.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="skipvideo" mnemonicParsing="false" onAction="#skip10" text="+10s">
                           <HBox.margin>
                              <Insets left="3.0" top="7.0" />
                           </HBox.margin>
                        </Button>
                        <Slider fx:id="volumeSlider">
                           <HBox.margin>
                              <Insets left="9.0" top="10.0" />
                           </HBox.margin>
                        </Slider>
                        <Button fx:id="ret" mnemonicParsing="false" onAction="#retour" text="retour" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </StackPane>
   </center>
</BorderPane>
